{
	"name": "vscode-ebnf",
	"version": "1.0.4",
	"publisher": "OMKOV",
	"engines": { "vscode": "^1.0.0" },
	"license": "MIT",
	"displayName": "EBNF",
	"description": "EBNF for Visual Studio Code",
	"categories": [ "Programming Languages" ],
	"keywords": [ "bnf", "ebnf" ],
	"contributes": {
		"languages": [
			{
				"id": "ebnf",
				"extensions": [ ".ebnf" ],
				"aliases": [ "EBNF" ],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ebnf",
				"scopeName": "text.ebnf",
				"path": "./syntaxes/ebnf.tmLanguage.json"
			},
			{
      	"scopeName": "markdown.ebnf.codeblock",
      	"path": "./syntaxes/ebnf.mdCodeblock.json",
      	"injectTo": ["text.html.markdown"],
      	"embeddedLanguages": {
        	"meta.embedded.block.ebnf": "ebnf"
      	}
    	}
		]
	},
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/Jamozed/vscode-ebnf"
	}
}
