{
	"name": "vscode-ebnf",
	"version": "1.1.0-alpha.1",
	"publisher": "OMKOV",
	"engines": { "vscode": "^1.0.0" },
	"license": "OLPE",
	"displayName": "EBNF",
	"description": "EBNF for Visual Studio Code",
	"categories": [ "Programming Languages" ],
	"keywords": [ "bnf", "ebnf" ],
	"contributes": {
		"languages": [
			{
				"id": "ebnf",
				"extensions": [ ".ebnf" ],
				"aliases": [ "EBNF" ],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ebnf",
				"scopeName": "text.ebnf",
				"path": "./syntaxes/ebnf.tmLanguage.json"
			}
		]
	},
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/Jamozed/vscode-ebnf"
	},
	"activationEvents": [ "onLanguage:ebnf" ],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.0.0",
		"@types/vscode": "^1.57.1",
		"typescript": "^4.3.5"
	}
}
